_zimfw_clean_compiled() {
  setopt LOCAL_OPTIONS PIPE_FAIL
  local find_opt rm_opt
  if [[ ${1} != -q ]]; then
    find_opt='-print'
    rm_opt='-v'
  fi
  command find ${ZIM_HOME} \( -name '*.zwc' -o -name '*.zwc.old' \) -delete ${find_opt} || return 1
  command rm -f ${rm_opt} <%= home %>/.zshrc.zwc{,.old} || return 1
  if [[ ${1} != -q ]]; then
    print -P "%F{green}âœ“%f Done with clean-compiled. Run %Bzimfw compile%b to re-compile."
  fi
}
