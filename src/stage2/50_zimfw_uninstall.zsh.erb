_zimfw_uninstall() {
  local zopt
  if (( _zprintlevel > 0 )) zopt='-v'
  if (( ${#_zunuseds} )); then
    if (( _zprintlevel <= 0 )) || read -q "?Uninstall ${#_zunuseds} module(s) listed above [y/N]? "; then
      _zimfw_print
      command rm -rf ${zopt} ${ZIM_HOME}/modules/${^_zunuseds} || return 1
    fi
  fi
  _zimfw_print -P '<%= done %>Done with uninstall.'
}
